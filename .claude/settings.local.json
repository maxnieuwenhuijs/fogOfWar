{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(mkdir:*)",
      "Bash(cp:*)",
      "Bash(npm install)",
      "Bash(npm start)",
      "Bash(grep:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git init:*)",
      "Bash(git remote add:*)",
      "Bash(npm start)",
      "Bash(npm run dev:*)",
      "Bash(rm:*)",
      "Bash(git push:*)",
      "Bash(for file in *.jsonl)",
      "Bash(done)",
      "Bash(curl:*)",
      "Bash(dir:*)"
    ],
    "deny": [],
    "defaultMode": "acceptEdits"
  }
}